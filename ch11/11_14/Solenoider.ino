boolean hihats[] = {1, 0, 0, 0, 0, 1, 0, 1};boolean snare[] = {1, 0, 0, 0, 1, 0, 0, 0};int hatPin   = 2;int snarePin = 3;int counter;void setup() {    counter = 0;}void loop() {    digitalWrite(hatPin,   hihats[counter]);    digitalWrite(snarePin, snare[counter]);     if(counter == 7) {        counter = 0;    } else {        counter++;    }    delay(200); // delay between beats}