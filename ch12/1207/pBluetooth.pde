import bluetoothDesktop.*Bluetooth bluetoothInstance;void setup() {    bluetoothInstance= new Bluetooth(this);    bluetoothInstance.discover();    noLoop(); // tell the Processing application that we don't need a loop}void deviceDiscoverEvent(Device dev) {    println(dev.name + " discovered.");}void deviceDiscoveryCompleteEvent(Device[] devices) {    println(" Found " + devices.length+ " devices.");    for( int i = 0; i<devices.length; i++) {    println("i: "+devices[i].name;    }}